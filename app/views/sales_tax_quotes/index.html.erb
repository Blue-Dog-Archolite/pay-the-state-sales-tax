<h1> All Requested Rates </h1>
<ul>
  <% @quotes.each do |quote|%>
    <%= link_to(quote) do %>
      <li>
        <p>
          Zip: <%= quote.zip%>
        </p>
        <p>
          Country: <%= quote.country%>
        </p>
      </li>
    <% end %>
  <% end %>
</ul>

<div>
  <%= link_to "New Quote", new_sales_tax_quote_url %>
</div>
